{
  "expo": {
    "name": "PumpGames",
    "slug": "PumpGames",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/ui/logo_mini.png",
      "resizeMode": "contain",
      "backgroundColor": "#F8F4E8"
    },
    "androidStatusBar": {
      "backgroundColor": "#F8F4E8",
      "translucent": false
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#F8F4E8"
      },
      "package": "com.rpalazkou.PumpGames"
    },
    "web": {
      "favicon": "./assets/ui/logo_mini.png"
    },
    "extra": {
      "eas": {
        "projectId": "7cdcd244-7b26-4b32-9cb2-b60f4580c9e0"
      }
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./app/assets/fonts/CrimsonPro-Regular.ttf",
            "./app/assets/fonts/DMSans_Medium.ttf",
            "./app/assets/fonts/DMSans_SemiBold.ttf"
          ]
        }
      ]
    ]
  }
}
